{
  "name": "td",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "front": "webpack-dev-server --config ./webpack.config.development.js",
    "prebuild": "rm -rf ./public/javascripts/* ./public/stylesheets/app.css",
    "build": "webpack -p --config ./webpack.config.production.js",
    "postbuild": "mv ./public/javascripts/app.css ./public/stylesheets/app.css",
    "solc": "solc --hashes ./contracts/contracts/CardMaster.sol -o ./contracts/build/signatures --overwrite",
    "update-contracts": "git submodule update --recursive --remote && (cd contracts && truffle compile && truffle migrate --reset) && npm run solc"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-riot": "^1.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "bignumber.js": "^4.0.2",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-beta.2",
    "cross-env": "^5.0.1",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.4",
    "debug": "~2.6.3",
    "es6-deferred": "^1.3.0",
    "ethers": "^2.1.1",
    "express": "~4.15.2",
    "express-session": "^1.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "firebase": "^4.1.3",
    "firebase-admin": "^5.0.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "morgan": "~1.8.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-easy-import": "^2.1.0",
    "postcss-loader": "^2.0.6",
    "postcss-short": "^4.1.0",
    "pug": "^2.0.0-rc.2",
    "raw-loader": "^0.5.1",
    "riot": "^3.6.0",
    "riot-route": "^3.1.2",
    "riotjs-loader": "^4.0.0",
    "serve-favicon": "~2.4.2",
    "spark-md5": "^3.0.0",
    "spectre.css": "^0.4.3",
    "style-loader": "^0.18.2",
    "superagent": "^3.5.2",
    "utf8": "^2.1.2",
    "web3": "^0.20.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1"
  },
  "devDependencies": {
    "uglifyjs-webpack-plugin": "^0.4.6"
  }
}
